package shape;

public class Command2 {
	public static void main(String[] args) {

		 float score=0;
		String course;
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the shape color: ");
		course = sc.next();
		System.out.print("Enter the squareâ€™s side: ");
		score = sc.nextFloat();

		Square John = new Square();

		John.setScore(course, score);
		John.computeGrade();

		System.out.println("You get Grade " + John.getGrade() + " in " + John.getCourse());
		 }

}
