package shape;

public class Square extends Shape{
private float area, perimeter;
	public void setArea1() {
		// TODO Auto-generated method stub
		area = shape * shape;
	}
	public float getArea(){
		return area;
	}
	public void setArea2() {
		perimeter = shape * 4;
	}
	public float getPerimeter(){
		return perimeter;
	}
	
}
