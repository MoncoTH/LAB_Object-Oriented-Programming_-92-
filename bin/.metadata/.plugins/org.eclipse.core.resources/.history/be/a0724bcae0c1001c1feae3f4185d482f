package test;

import java.util.Scanner;

public class command1 {
    public static void main(String[] args) {
        CDAccount acc1 = new CDAccount();
        acc1.setBalance(6000.00);
        System.out.println("1:Balance =%f\n",acc1.checkBalance());
        acc1.deposit(1500.00);
        acc1.deposit(500.00);
        System.out.println("2:Balance =%f\n",acc1.checkBalance());
        acc1.calinterest();
        System.out.println("3:Balance =%f\n",acc1.getInterest());
        acc1.withdraw(900.00);
        System.out.println("4:Balance =%f\n",acc1.checkBalance());
    }
}
